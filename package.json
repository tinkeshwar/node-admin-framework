{
  "name": "node-backend",
  "version": "2.0.0",
  "description": "Node Backed Service",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon src/index.ts ts-node-dev src/index.ts",
    "start": "node .",
    "worker": "node dist/config/worker.js",
    "worker-dev": "cross-env NODE_ENV=development ts-node-dev src/config/worker.ts",
    "new:module": "./node_modules/.bin/hygen entity new",
    "new:model": "./node_modules/.bin/hygen entity model",
    "reset:db": "./node_modules/.bin/sequelize-cli db:seed:undo:all && ./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "new:migration": "./node_modules/.bin/sequelize-cli migration:generate --name",
    "new:seeder": "./node_modules/.bin/sequelize-cli seed:generate --name",
    "migrate": "./node_modules/.bin/sequelize-cli db:migrate",
    "rollback": "./node_modules/.bin/sequelize-cli db:migrate:undo",
    "seed:all": "./node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "./node_modules/.bin/sequelize-cli db:seed:undo",
    "setup:db": "./node_modules/.bin/sequelize-cli db:migrate && ./node_modules/.bin/sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tinkeshwar/node-admin-framework.git"
  },
  "author": "Tinkeshwar Singh",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/hapi": "^20.1.3",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.1.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "bull": "^3.22.7",
    "busboy": "^0.3.1",
    "camelcase": "^6.2.0",
    "camelcase-keys": "^6.2.2",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^3.3.1",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "hapi-auth-jwt2": "^10.2.0",
    "ioredis": "^4.27.4",
    "joi": "^17.2.1",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "mysql2": "^2.2.5",
    "node-cleanup": "^2.1.2",
    "path": "^0.12.7",
    "pidusage": "^2.0.21",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "snakecase-keys": "^3.2.0",
    "to-snake-case": "^1.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/bluebird": "^3.5.35",
    "@types/bull": "^3.15.1",
    "@types/busboy": "^0.2.3",
    "@types/camelcase-keys": "^5.1.1",
    "@types/express": "^4.17.12",
    "@types/fs-extra": "^9.0.11",
    "@types/hapi__hapi": "^20.0.8",
    "@types/hapi__inert": "^5.2.1",
    "@types/hapi__joi": "^17.1.6",
    "@types/hapi__vision": "^5.5.2",
    "@types/ioredis": "^4.26.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.170",
    "@types/mailgun-js": "^0.22.11",
    "@types/node": "^14.17.2",
    "@types/node-cleanup": "^2.1.1",
    "@types/pidusage": "^2.0.1",
    "@types/pug": "^2.0.4",
    "@types/shelljs": "^0.8.8",
    "@types/to-snake-case": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "fs-extra": "^9.0.1",
    "hapi-swagger": "^14.1.3",
    "hygen": "^6.1.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  }
}
