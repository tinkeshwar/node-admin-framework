{
  "name": "saga-admin",
  "version": "1.0.0",
  "description": "Vidhyasaga admin support tool",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon src/index.ts ts-node-dev src/index.ts",
    "start": "node .",
    "entity:generate": "./node_modules/.bin/hygen entity new",
    "reset:db": "./node_modules/.bin/sequelize-cli db:seed:undo:all && ./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "create:migration": "./node_modules/.bin/sequelize-cli migration:generate --name",
    "create:seeder": "./node_modules/.bin/sequelize-cli seed:generate --name",
    "migrate": "./node_modules/.bin/sequelize-cli db:migrate",
    "rollback": "./node_modules/.bin/sequelize-cli db:migrate:undo",
    "seed:all": "./node_modules/.bin/sequelize-cli db:seed:all",
    "setup:db": "./node_modules/.bin/sequelize-cli db:migrate && ./node_modules/.bin/sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://tinkeshwar@bitbucket.org/vidhyasaga/admin.git"
  },
  "author": "Tinkeshwar Singh",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/hapi": "^20.0.1",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.0.1",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "camelcase-keys": "^6.2.2",
    "crypto-random-string": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "hapi-auth-jwt2": "^10.2.0",
    "ioredis": "^4.24.3",
    "joi": "^17.2.1",
    "lodash": "^4.17.20",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "snakecase-keys": "^3.2.0",
    "to-snake-case": "^1.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/camelcase-keys": "^5.1.1",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.2",
    "@types/hapi__hapi": "^20.0.1",
    "@types/hapi__inert": "^5.2.1",
    "@types/hapi__joi": "^17.1.6",
    "@types/hapi__vision": "^5.5.2",
    "@types/ioredis": "^4.22.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.2",
    "@types/shelljs": "^0.8.8",
    "@types/to-snake-case": "^1.0.0",
    "@types/validator": "^13.1.0",
    "fs-extra": "^9.0.1",
    "hapi-swagger": "^14.0.0",
    "hygen": "^6.0.4",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
